#!/bin/bash

# Ultra-fast AI Analyzer for Google Colab
# This version is optimized for maximum speed on Colab's limited resources

echo "🚀 Starting ultra-fast AI analysis for Google Colab..."
echo "⚡ Optimized for speed: 512x512 images, 60% quality"
echo "📦 Batch size: 5, Workers: 2"

if [ $# -eq 0 ]; then
    echo "❌ Please provide a property ID"
    echo "Usage: ./ai-analyzer-fast <property-id>"
    echo "Example: ./ai-analyzer-fast 30434108090030050"
    exit 1
fi

PROPERTY_ID=$1

echo "🏠 Processing property: $PROPERTY_ID"

# Run the ultra-fast analyzer
python -m src.ai_image_analysis_colab_fast --property-id "$PROPERTY_ID"

echo "✅ Ultra-fast analysis completed!" 